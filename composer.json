{
    "name": "terminal42/webling-bundle",
    "description": "Symfony bundle for our Webling API client",
    "type": "symfony-bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "https://www.terminal42.ch/"
        }
    ],
    "funding": [
        {
        "type": "github",
        "url": "https://github.com/terminal42"
        },
        {
            "type": "other",
            "url": "https://ko-fi.com/terminal42"
        }
    ],
    "support": {
        "issues": "https://github.com/terminal42/webling-bundle/issues",
        "source": "https://github.com/terminal42/webling-bundle"
    },
    "require": {
        "php": "^7.1 || ^8.0",
        "terminal42/webling-api": "^2.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/config": "^4.4 || ^5.0"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^3.0",
        "symfony/http-client": "^4.4 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\WeblingBundle\\": "src"
        }
    },
    "scripts": {
        "cs-fixer": [
            "vendor/bin/ecs check src/ --fix --ansi"
        ]
    }
}
