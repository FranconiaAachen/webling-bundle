{
    "name": "terminal42/webling-bundle",
    "description": "Symfony2 bundle for our Webling API client",
    "type": "symfony-bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "https://www.terminal42.ch/"
        }
    ],
    "require": {
        "terminal42/webling-api": "^1.0",
        "symfony/http-kernel": "~2.7 || ^3.0",
        "symfony/dependency-injection": "~2.7 || ^3.0",
        "symfony/config": "~2.7 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\WeblingBundle\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
